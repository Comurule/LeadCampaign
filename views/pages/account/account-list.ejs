<div class="col-sm-12">    
    <div class="">
        
        <table style="width:100%; text-align:left; padding:1px;" border= "1">
            <tr>
                <th style="text-align:center;">ID</th>
                <th style="text-align:center;">First Name</th>
                <th style="text-align:center;">Last Name</th>
                <th style="text-align:center;">Email</th>
                <th style="text-align:center;">Preferences (respective Tiers)</th>
                <th></th>
            </tr>
            <% accounts.forEach(function(account) { %>
                <a href="/main/accounts/<%= account.id %>">
                    <tr>
                        <td style="text-align:center"><%= account.id %></a></td>
                        <td style="text-align:center"><%= account.firstName %></td>
                        <td style="text-align:center"><%= account.lastName %></td>
                        <td style="text-align:center"><a href="/main/accounts/<%= account.id %>"><%= account.email %></a></td>
                        <td style="text-align:center">
                            
                                <% account.PreferenceCenters.forEach(preference => { %>
                                    <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i>
                                    <li><%=preference.name%> (<%=preference.tier%>)</li>   
                                <% }) %>
                        </td>
                        <td style="text-align:center;"> 
                            <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i><li><a href="/main/accounts/<%= account.id %>/update">Update Account</a></li>
                            <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i><li><a href="/main/accounts/<%= account.id %>/delete">Delete Account</a></li>
                         </td>
                    </tr>
                </a>
            <% }); %> 
        </table>
    </div>
</div>
 
 