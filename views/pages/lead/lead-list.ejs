<div class="col-sm-12">    
    <div class="">
            
        <table style="width:100%; text-align:left; padding:1px;" border= "1">
            <tr>
                <th style="text-align:center;">ID</th>
                <th style="text-align:center;">First Name</th>
                <th style="text-align:center;">Last Name</th>
                <th style="text-align:center;">Email</th>
                <th style="text-align:center;">Preferences (respective Tiers)</th>
                <th></th>
            </tr>
            <% leads.forEach(function(lead) { %>
                <a href="/main/leads/<%= lead.id %>">
                    <tr>
                        <td style="text-align:center"><%= lead.id %></a></td>
                        <td style="text-align:center"><%= lead.firstName %></td>
                        <td style="text-align:center"><%= lead.lastName %></td>
                        <td style="text-align:center"><a href="/main/leads/<%= lead.id %>"><%= user.email %></a></td>
                        <td style="text-align:center">
                            
                                <% lead.PreferenceCenters.forEach(preference => { %>
                                    <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i>
                                    <li><%=preference.name%> (<%=preference.tier%>)</li>   
                                <% }) %>
                        </td>
                        <td style="text-align:center;"> 
                            <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i><li><a href="/main/leads/<%= lead.id %>/update">Update Lead</a></li>
                            <i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i><li><a href="/main/leads/<%= lead.id %>/delete">Delete Lead</a></li>
                            
                                <form action="/main/accounts/create">
                                    <input type="hidden" name="leadId" value="<%= lead.id %>">
                                    <input type="submit" value="Convert Lead" class="btn btn-link">
                                </form>
                            
                        </td>
                    </tr>
                </a>
            <% }); %> 
        </table>
    </div>
</div>
 
 